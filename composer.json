{
    "name": "shishaoqi/meituan-flash-purchase",
    "description": "美团外卖开放平台PHP SDK，更准确的说是美团闪购 SDK",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": ">=6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.20"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "shishaoqi",
            "email": "shishaoqi0928@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Shishaoqi\\MeituanFlashPurchase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shishaoqi\\MeituanFlashPurchase\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
